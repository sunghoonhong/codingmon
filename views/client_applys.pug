extends layout

block content
  h2 프리랜서
  table
    thead
      tr
        th ID
        th 이름
        th 휴대폰
        th 나이
        th 전공
        th 경력
        th 평점
    tbody
      for freelancer in freelancers
        tr
          td
            a(href='/profile/'+freelancer.id)= freelancer.id
          td= freelancer.name
          td= freelancer.phone_num
          td= freelancer.age
          td= freelancer.major
          td= freelancer.career
          td= freelancer.rating
  h2 팀